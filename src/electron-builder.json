{
	"appId": "com.example.myapp",
	"productName": "Markdown Viewer",
	"directories": {
		"output": "../build/Markdown_Viewer"
	},
	"files": [
		"index.js",
		"*.js",
		"package.json",
		"modules/**/*",
		{
			"from": "html/**/*",
			"to": "html"
		}
	],
	"extraResources": [
		{
		"from": "html",
		"to": "html",
		"filter": ["**/*"]
		}
	]
}
